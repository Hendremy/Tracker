<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Hendricé.Rémy.Poo.Tracker.Gui.JobConflictControl">
	<Border BorderThickness="3" BorderBrush="DarkMagenta" CornerRadius="10">
		<WrapPanel Orientation="Horizontal">
			<StackPanel>
				<StackPanel Orientation="Horizontal">
					<TextBlock Name="PlanningA"></TextBlock>
					<TextBlock Name="NameA"></TextBlock>
				</StackPanel>
				<StackPanel Orientation="Horizontal">
					<TextBlock Name="PlanningB"></TextBlock>
					<TextBlock Name="NameB"></TextBlock>
				</StackPanel>
			</StackPanel>
			<StackPanel>
				<ItemsControl Name="Dates">
					
				</ItemsControl>
			</StackPanel>
		</WrapPanel>
	</Border>
</UserControl>
